<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Group admin #<%= @group_admin.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/group_admins" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link group_admin start -->
      <div class="col">
        <a href="/group_admins/<%= @group_admin.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit group admin
        </a>
      </div>
      <!-- Edit link group_admin end -->

      <!-- Delete link group_admin start -->
      <div class="col">
        <a href="/delete_group_admin/<%= @group_admin.id %>" class="btn btn-block btn-outline-secondary">
          Delete group admin
        </a>
      </div>
      <!-- Delete link group_admin end -->
    </div>

    <dl>
      <dt>
        User
      </dt>
      <!-- Display user_id start -->
      <dd>
        <%= @group_admin.user_id %>
      </dd>
      <!-- Display user_id end -->

      <dt>
        Group
      </dt>
      <!-- Display group_id start -->
      <dd>
        <%= @group_admin.group_id %>
      </dd>
      <!-- Display group_id end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@group_admin.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@group_admin.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<!-- Show Page Customization end -->
