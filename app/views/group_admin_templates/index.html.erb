<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All group admins
    </h1>

    <a href="/group_admins/new" class="btn btn-block btn-outline-secondary">
      Add a new group admin
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          User
        </th>

        <th>
          Group
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @group_admins.each do |group_admin| %>
      <tr>
        <!-- Display group_admin.id start -->
        <td>
          <%= group_admin.id %>
        </td>
        <!-- Display group_admin.id end -->

        <!-- Display group_admin.user_id start -->
        <td>
          <%= group_admin.user_id %>
        </td>
        <!-- Display group_admin.user_id end -->

        <!-- Display group_admin.group_id start -->
        <td>
          <%= group_admin.group_id %>
        </td>
        <!-- Display group_admin.group_id end -->

        <!-- Display group_admin.created_at start -->
        <td>
          <%= time_ago_in_words(group_admin.created_at) %> ago
        </td>
        <!-- Display group_admin.created_at end -->

        <!-- Display group_admin.updated_at start -->
        <td>
          <%= time_ago_in_words(group_admin.updated_at) %> ago
        </td>
        <!-- Display group_admin.updated_at end -->

        <td>
          <a href="/group_admins/<%= group_admin.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
